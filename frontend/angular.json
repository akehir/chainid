{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "company": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/company",
      "sourceRoot": "projects/company/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/company",
            "index": "projects/company/src/index.html",
            "main": "projects/company/src/main.ts",
            "polyfills": "projects/company/src/polyfills.ts",
            "tsConfig": "projects/company/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/company/src/favicon.ico",
              "projects/company/src/assets"
            ],
            "styles": [
              "projects/company/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/company/src/environments/environment.ts",
                  "with": "projects/company/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "company:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "company:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "company:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/company/src/test.ts",
            "polyfills": "projects/company/src/polyfills.ts",
            "tsConfig": "projects/company/tsconfig.spec.json",
            "karmaConfig": "projects/company/karma.conf.js",
            "assets": [
              "projects/company/src/favicon.ico",
              "projects/company/src/assets"
            ],
            "styles": [
              "projects/company/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/company/tsconfig.app.json",
              "projects/company/tsconfig.spec.json",
              "projects/company/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/company/e2e/protractor.conf.js",
            "devServerTarget": "company:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "company:serve:production"
            }
          }
        }
      }
    },
